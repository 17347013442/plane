{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "express",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src", "next-env.d.ts"],
  "exclude": ["node_modules"]
}
